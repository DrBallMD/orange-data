{
    "name": "drballmd/orange-data",
    "description": "PHP implementation of Orange Data API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Anikeev",
            "email": "anikeev.dmitry@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ramsey/uuid": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "~1.0",
        "phpseclib/phpseclib": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0 || ^8.0"
    },
    "suggest": {
        "jms/serializer": "^1.7",
        "mhujer/jms-serializer-uuid": "^1.0",
        "ramsey/uuid-doctrine": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "OrangeData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OrangeData\\Tests\\": "tests/"
        }
    }
}
